<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.mozeshajdu.audiotagcollector.view.MainView"
            prefHeight="400.0" prefWidth="600.0">
    <HBox>
        <Button fx:id="dragAndDropButton" text="Drop files" onAction="#dragAndDropButtonClicked"/>
        <Button fx:id="logButton" text="View logs" onAction="#logButtonClicked"/>
    </HBox>
</AnchorPane>
